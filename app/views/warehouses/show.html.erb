<h1> Galpao <%= @warehouse.code %></h1>
<h3><%= @warehouse.description %></h3>
<div>
  <p><strong>Nome: </strong> <%= @warehouse.name %></p>
  <p><strong>Cidade: </strong> <%= @warehouse.city %></p>
  <p><strong>Area: </strong> <%= @warehouse.area %> m2</p>
  <p><strong>Endereco: </strong> <%= @warehouse.address %> <strong>CEP: </strong> <%= @warehouse.zip_code %></p>
</div>

<div id = 'links'>
<%= link_to('Editar', edit_warehouse_path(@warehouse)) %>
<%= link_to('Voltar', '/') %>
</div>