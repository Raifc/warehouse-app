<h1>Novo Galpao</h1>

<% if (@warehouse.errors.any?) %>
  <p>Verifique os erros abaixo:</p>
  <ul>
    <% @warehouse.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>

<%= form_with(model: @warehouse) do |f| %>
  <div id='name'>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div id='description'>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div id='code'>
    <%= f.label :code %>
    <%= f.text_field :code %>
  </div>

  <div id='address'>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div id='city'>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div id='zip_code'>
    <%= f.label :zip_code %>
    <%= f.text_field :zip_code %>
  </div>

  <div id='area'>
    <%= f.label :area %>
    <%= f.number_field :area %>
  </div>

  <div id='submit'>
    <%= f.submit 'Enviar' %>
  </div>


<% end %>