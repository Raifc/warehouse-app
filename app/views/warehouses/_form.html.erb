<%= form_with(model: @warehouse) do |f| %>
  <div class = 'form_fields'>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class = 'form_fields'>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div class = 'form_fields'>
    <%= f.label :code %>
    <%= f.text_field :code %>
  </div>

  <div class = 'form_fields'>
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div class = 'form_fields'>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class = 'form_fields'>
    <%= f.label :zip_code %>
    <%= f.text_field :zip_code %>
  </div>

  <div class = 'form_fields'>
    <%= f.label :area %>
    <%= f.number_field :area %>
  </div>

  <div id='submit'>
    <%= f.submit 'Enviar' %>
  </div>

<% end %>